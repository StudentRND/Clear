{% extends "template.twig" %}
{% block title %}Attendee Finder{% endblock %}
{% block section %}search{% endblock %}
{% import "widgets/attendees.twig" as a %}
{% block content %}
    <h2>Search: {{ search }} <span class="confidential">Confidential</span></h2>   
    {% for batch in batches %}
        <h3>{{ batch.batch.name }}</h3>
        {{ a.attendees(batch.results) }}
    {% endfor %}
{% endblock %}
