{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "url": "https://github.com/StudentRND/php-ups-api.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "rcrowe/twigbridge": "0.9.0",
        "studentrnd/s5api": "dev-master",
        "stripe/stripe-php": "dev-master",
        "pda/pheanstalk": "3.1.*",
        "guzzlehttp/guzzle": "6.*",
        "twilio/sdk": "~4.2",
        "aws/aws-sdk-php": "3.*",
        "tecnickcom/TCPDF": "6.2.*",
        "tylermenezes/legalesign-php": "0.9.3",
        "sendgrid/sendgrid": "^4.0",
        "tijsverkoyen/css-to-inline-styles": "^1.5",
        "soundasleep/html2text": "^0.3.1",
        "jbdemonte/barcode": "^2.1",
        "eo/passbook": "^1.3",
        "mindscape/raygun4php": "1.*",
        "userscape/customerio": "^1.0",
        "setasign/fpdi": "1.6.1",
        "setasign/fpdi-fpdf": "1.6.1",
        "spatie/pdf-to-image": "^1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.7.*|3.0.*",
        "symfony/dom-crawler": "2.7.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "CodeDay\\Clear\\Tests\\": "tests/",
            "CodeDay\\Clear\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
